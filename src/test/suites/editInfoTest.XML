<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Simple test suite">
    <parameter name="dataFile" value="src/test/data/data.json"/>
    <parameter name="dataToReplaceFile" value="src/test/data/dataToReplace.json"/>


    <test name="Some Test" verbose="2">
        <parameter name="oldPassword" value="12345"></parameter>
        <parameter name="newPassword" value="54321"></parameter>
        <classes>
            <class name="EditTest">
                <methods>
                    <include name="getOldPasswordParameter" />
                    <include name="getNewPasswordParameter" />
                    <include name="verifyOldPersonalInformation" />
                    <include name="editOldPersonalInformation" />
                    <include name="verifyNewPersonalInformation" />
                    <include name="editNewPersonalInformation" />
                </methods>
            </class>
        </classes>
    </test>

</suite>